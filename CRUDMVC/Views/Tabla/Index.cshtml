@model List<CRUDMVC.Models.ViewModels.ListTablaViewModel>

@{
    ViewBag.Title = "DATOS REGISTRO";
}

<h2> @ViewBag.Title </h2>
<div class="row" style="text-align:right;padding:5px;"> 
    <a href="/Tabla/Nuevo" class="btn btn-primary">Nuevo Registro</a>
</div>
<div class="row">
    
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>#</th>
                <th>Nombres Apellidos</th>
                <th>Correo Electronico</th>
                <th>Fecha Nacimiento</th>
                <th></th>
            </tr>
            @foreach (var Oelemnto in Model)
            {
        <tr>
            <td>@Oelemnto.Id</td>
            <td>@Oelemnto.Nombre</td>
            <td>@Oelemnto.Correo</td>
            <td>@Oelemnto.fecha_Nacimiento</td>
            <td>
                <a class="btn btn-default"href="~/Tabla/Editar/@Oelemnto.Id">Editar</a>
                <a class="btn btn-danger"href="~/Tabla/Eliminar/@Oelemnto.Id">Eliminar</a>
            </td>

        </tr>
            }

        </table>
    </div>

</div>

